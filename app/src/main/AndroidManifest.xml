<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aaexample.restaurant">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".classes.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Restaurant">


        <activity
            android:name=".Igrediant"
            android:label="@string/title_activity_igrediant"></activity>
        <activity android:name=".Item" />
        <activity android:name=".Categories" />
        <activity android:name=".Tables" />
        <activity android:name=".Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity"/>


    </application>

</manifest>